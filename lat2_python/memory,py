class soccer():

    __club = ""
    __country = ""
    __year = ""
    __player = ""
    __stadium = ""

    def __init__(self, club = "", country = "", year = "", player = "", stadium = ""):
        self.__club = club
        self.__country = country
        self.__year = year
        self.__player = player
        self.__stadium = stadium

    def setClub(self, club):
        self.__club = club

    def setCountry(self, country):
        self.__country = country

    def setYear(self, year):
        self.__year = year

    def setPlayer(self, player):
        self.__player = player

    def setStadium(self, stadium):
        self.__stadium = stadium

    def getClub(self):
        return self.__club

    def getCountry(self):
        return self.__country

    def getYear(self):
        return self.__year

    def getPlayer(self):
        return self.__player

    def getStadium(self):
        return self.__stadium 